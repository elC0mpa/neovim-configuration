so ~/.config/nvim/vim-plug/plugins.vim
so ~/.config/nvim/maps.vim

set rnu
set number
set splitright

syntax on
colorscheme gruvbox
set termguicolors

let g:gruvbox_contrast_dark = "hard"
let g:coc_global_extensions = ['coc-emmet', 'coc-css', 'coc-html', 'coc-json', 'coc-prettier', 'coc-tsserver', 'coc-vetur', 'coc-yank', 'coc-tslint-plugin']



